service_key=$(cat /sv/internal/jenkins-gce.json)

docker run -v /sv/applications/sv-kubernetes-example:/repo \
	-v /sv/containers/sv-deploy-gce/lib:/app/lib \
	-v /sv/containers/sv-deploy-gce/scripts:/app/scripts \
	-v /var/run/docker.sock:/var/run/docker.sock \
	-e PROJECT_ID=sv-shared-231700 \
	-e REPO_NAME=sv-kubernetes-example \
	-e BRANCH_NAME=master \
	-e ZONE=us-east1-b \
	-e GCLOUD_SERVICE_KEY="$service_key" \
	-it sv-deploy-gce /bin/bash